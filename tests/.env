# Deployment information
# The deployment namespace is retrieved from the `serviceaccount` volume
#export ZENKO_K8S_NAMESPACE=
# The Zenko Helm release name
#export ZENKO_HELM_RELEASE=

# Zenko S3 access credentials
#export AWS_ACCESS_KEY_ID=
#export AWS_SECRET_ACCESS_KEY=

# Service URLs
# These are derived from `ZENKO_HELM_RELEASE` if set
test -z "${CLOUDSERVER_FRONT_ENDPOINT:-}" && export CLOUDSERVER_FRONT_ENDPOINT=http://${ZENKO_HELM_RELEASE}-cloudserver-front:80
test -z "${PROMETHEUS_ENDPOINT:-}" && export PROMETHEUS_ENDPOINT=http://${ZENKO_HELM_RELEASE}-prometheus-server:80
